#: import Button kivy.uix.button.Button

<logtime>:
    hit1: hit_button1
    hit2: hit_button2
    hit3: hit_button3
    timel: time_label

    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size

    Button:
        id: hit_button1
        text: "Hit1"
        font_size: 20
        size: root.width/4, root.width/4
        center_x: (root.width/6)
        center_y: ((root.height/4)*3)
        on_press: app.time_get("hit1")

    Button:
        id: hit_button2
        text: "Hit2"
        font_size: 20
        size: root.width/4, root.width/4
        center_x: (root.width/2)
        center_y: ((root.height/4)*3)
        on_press: app.time_get("hit2")

    Button:
        id: hit_button3
        text: "Hit3"
        font_size: 20
        size: root.width/4, root.width/4
        center_x: ((root.width/6)*5)
        center_y: ((root.height/4)*3)
        on_press: app.time_get("hit3")

    Label:
        id: time_label
        text: "Last_Log"
        color: [0, 0, 0, 1]
        font_size: 20
        size: root.width/2, root.height/2
        center_x: (root.width/2)
        center_y: (root.height/4)

